# 应用配置
PORT=8080
SPRING_PROFILES_ACTIVE=dev

# 数据库配置
MYSQL_DATABASE=campus_repair
MYSQL_USER=repair_user
MYSQL_PASSWORD=your_mysql_password

# Redis配置
SPRING_REDIS_HOST=localhost
REDIS_PASSWORD=your_redis_password

# RabbitMQ配置
SPRING_RABBITMQ_HOST=localhost
RABBITMQ_USER=repair_user
RABBITMQ_PASSWORD=your_rabbitmq_password

# JWT配置
JWT_SECRET=your_jwt_secret_key

# 文件上传配置
UPLOAD_PATH=./uploads

# 开发环境配置
# 是否启用Swagger
DEV_SWAGGER_ENABLED=true
# 是否打印SQL
DEV_SHOW_SQL=true
# 是否允许跨域
DEV_CORS_ENABLED=true
# 是否启用Redis缓存
DEV_CACHE_ENABLED=true
# 是否启用RabbitMQ
DEV_MQ_ENABLED=true
# 是否启用文件上传
DEV_UPLOAD_ENABLED=true
# 是否启用邮件发送
DEV_MAIL_ENABLED=false
# 是否启用短信发送
DEV_SMS_ENABLED=false
# 是否启用验证码
DEV_CAPTCHA_ENABLED=false
# 是否启用WebSocket
DEV_WEBSOCKET_ENABLED=false
# 是否启用定时任务
DEV_SCHEDULER_ENABLED=false
# 是否启用异步任务
DEV_ASYNC_ENABLED=true
# 是否启用事务
DEV_TRANSACTION_ENABLED=true
# 是否启用AOP
DEV_AOP_ENABLED=true
# 是否启用监控
DEV_MONITOR_ENABLED=false

# 日志配置
LOGGING_LEVEL_ROOT=INFO
LOGGING_LEVEL_COM_ZSXYWW_BACKEND=DEBUG
LOGGING_LEVEL_ORG_SPRINGFRAMEWORK_WEB=DEBUG
LOGGING_LEVEL_ORG_SPRINGFRAMEWORK_SECURITY=DEBUG

# 注意：
# 1. 复制此文件为.env并修改配置
# 2. 生产环境中应该使用更安全的密码
# 3. JWT密钥应该使用足够长的随机字符串
# 4. 数据库密码应该使用强密码
# 5. Redis密码应该使用强密码
# 6. RabbitMQ密码应该使用强密码